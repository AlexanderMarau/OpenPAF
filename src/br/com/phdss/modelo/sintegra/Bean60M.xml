<?xml version="1.0" encoding="UTF-8"?>
<beanio xmlns="http://www.beanio.org/2011/01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.beanio.org/2011/01 http://www.beanio.org/2011/01/mapping.xsd">
    <stream name="SINTEGRA" format="fixedlength">
        <record name="Dados" minOccurs="1" maxOccurs="1" class="br.com.phdss.modelo.sintegra.Dados60M">
            <field name="tipo" type="int" length="2"/>
            <field name="subtipo" type="string" length="1" trim="true"/>
            <field name="data" type="date" length="8" justify="right" padding="0" format="yyyyMMdd"/>
            <field name="serie" type="string" length="20" justify="left" padding=" "/>
            <field name="caixa" type="int" length="3" justify="right" padding="0"/>
            <field name="modelo" type="string" length="2" justify="left" padding=" "/>
            <field name="cooInicial" type="int" length="6" justify="right" padding="0"/>
            <field name="cooFinal" type="int" length="6" justify="right" padding="0"/>
            <field name="crz" type="int" length="6" justify="right" padding="0"/>
            <field name="cro" type="int" length="3" justify="right" padding="0"/>
            <field name="valorBruto" type="double" length="16" format="#0" justify="right" padding="0"/>
            <field name="valorGeral" type="double" length="16" format="#0" justify="right" padding="0"/>
            <field name="brancos" type="string" length="37" justify="left" padding=" "/>
        </record>
    </stream>
</beanio>